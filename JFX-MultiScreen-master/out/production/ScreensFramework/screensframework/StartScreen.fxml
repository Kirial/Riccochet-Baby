<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" maxHeight="-1.0" maxWidth="-1.0" minHeight="720.0" minWidth="1280.0" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="screensframework.StartScreenController">
    <children>
        <HBox maxHeight="-1.0" maxWidth="-1.0" minHeight="720.0" minWidth="1280.0" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Region nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="ALWAYS" />
            <VBox fx:id="MenuContainer" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="200.0" prefWidth="100.0" scaleShape="false" spacing="20.0">
                <children>
                    <Button fx:id="PlayBtn" maxWidth="120.0" minWidth="200.0" mnemonicParsing="false" onAction="#goToPlayScreen" text="Play">
                        <padding>
                            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                        </padding>
                    </Button>
                    <Button fx:id="PrevPlayedBtn" maxWidth="120.0" minWidth="200.0" mnemonicParsing="false" onAction="#goToPrevPlayedScreen" text="Previously Played">
                        <padding>
                            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                        </padding>
                    </Button>
                    <Button fx:id="TopscoreBtn" maxWidth="120.0" minWidth="200.0" mnemonicParsing="false" text="Top Score">
                        <padding>
                            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                        </padding>
                    </Button>
                    <Button fx:id="HowToBtn" maxWidth="120.0" minWidth="200.0" mnemonicParsing="false" text="How To">
                        <padding>
                            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                        </padding>
                    </Button>
                    <Button fx:id="ExitBtn" maxWidth="120.0" minWidth="200.0" mnemonicParsing="false" onAction="#closeButtonAction" text="Exit">
                        <padding>
                            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                        </padding>
                    </Button>
                </children>
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </padding>
            </VBox>
            <Region HBox.hgrow="ALWAYS" />

        </HBox>
    </children>
</AnchorPane>
